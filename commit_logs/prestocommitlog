commit 3576a98d1a108e219bb6e6f2f0ced06394ff8bb5
Author: Martin Traverso <martint@fb.com>
Date:   Sat Dec 20 19:25:15 2014 -0800

    Generate local union when inputs are unpartitioned
    
    Before:
    
     Fragment 2 [SINGLE]
         Output layout: [expr_3]
         - Output[_col0] => [expr_3:bigint]
                 _col0 := expr_3
             - RemoteSource[0,1] => [expr_3:bigint]
    
     Fragment 0 [SINGLE]
         Output layout: [expr]
         - Project => [expr:bigint]
                 expr := 1
             - Values => []
                     ()
    
     Fragment 1 [SOURCE]
         Output layout: [orderkey]
         - TableScan[tpch:tpch:orders:sf1.0, original constraint=true] => [orderkey:bigint]
                 orderkey := tpch:tpch:orderkey:0
    
    After:
    
     Fragment 1 [SINGLE]
         Output layout: [expr_3]
         - Output[_col0] => [expr_3:bigint]
                 _col0 := expr_3
             - Union => [expr_3:bigint]
                 - Project => [expr:bigint]
                         expr := 1
                     - Values => []
                             ()
                 - RemoteSource[0] => [expr_3:bigint]
    
     Fragment 0 [SOURCE]
         Output layout: [orderkey]
         - TableScan[tpch:tpch:orders:sf1.0, original constraint=true] => [orderkey:bigint]
                 orderkey := tpch:tpch:orderkey:0

