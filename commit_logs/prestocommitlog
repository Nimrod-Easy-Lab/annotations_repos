commit 98124a4ef3efec195c79f4b50822993895c95f2d
Author: Nileema Shingte <nileema.shingte@gmail.com>
Date:   Mon May 11 21:50:46 2015 -0700

    Add ProjectionPushDown optimizer
    
    EXPLAIN
    SELECT day(ds), status
    FROM (
      SELECT orderdate ds, orderstatus status
      FROM orders
      UNION ALL
      SELECT shipdate ds, linestatus status
      FROM lineitem
    );
    
    Query Plan:
     - Output[_col0, status] => [day:bigint, orderstatus_12:varchar]
             _col0 := day
             status := orderstatus_12
         - Exchange[GATHER] => day:bigint, orderstatus_12:varchar
             - Project => [day_21:bigint, orderstatus:varchar]
                     day_21 := "day"("orderdate")
                 - TableScan[tpch:tpch:orders:sf0.01, original
    	     constraint=true] => [orderstatus:varchar, orderdate:date]
                         orderstatus := tpch:orderstatus:2
                         orderdate := tpch:orderdate:4
             - Project => [day_23:bigint, linestatus:varchar]
                     day_23 := "day"("shipdate")
                 - TableScan[tpch:tpch:lineitem:sf0.01, original
    	     constraint=true] => [linestatus:varchar, shipdate:date]
                         linestatus := tpch:linestatus:9
                         shipdate := tpch:shipdate:10

